.tue-offer-tpl.tue-bg-silver-25
    .container
        .offer-title.tue-title.tue-txt-center Las ofertas del mundo
        .offer-subtitle.tue-subtitle Despeja tus dudas, revisa nuestra lista de 
            a.offer-underline(href="#") preguntas frecuentes.
        .offer-row
            .offer-col
                .prv-card.prv-style-2
                    picture.prv-card__image
                        source(media="(min-width: 768px)", srcset="images/offer-1-desktop.png")
                        img(src="images/offer-1-mobile.png", alt="", width="324", height="126")
                    .prv-card__body
                        p.card__label POR MÁS TUENTIS
                        h3.card__title.title__ellipsis Refiere y gana
                        .card__button
                            a.tue-btn.tue-btn--purple.tue-btn--round(href="#") Referir amigos
