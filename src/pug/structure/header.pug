header.tuec__header 
    .container 
        .brand 
            picture 
                img(src="images/header/tuec_logo.png", alt="logo tuenti", srcset="images/header/tuec_logo.png")
        .hambMenu
            button(type="button" class="btn--menu" id="btnNav") 
                span.text Menu
                span.icon-left
                span.icon-right        
        nav.navigation
            ul.menu__list.accordion(data-multiselect="false")
                li.menu__item.has__child
                    a(href="http://", target="_blank", rel="noopener noreferrer", class="link accordion__tab") 
                        span.icon: img(src="images/header/icon-sim-card.png", alt="icono sim card", srcset="images/header/icon-sim-card.png")
                        span.text Pide tu chip
                        span.icon.icon--toggle: svg: use(xlink:href=iconPath+'chevron-down')
                    ul.submenu__list.accordion__panel
                        li.submenu__item: a(href="http://", target="_blank", rel="noopener noreferrer", class="link") 
                            span.icon: img(src="images/header/submenu/icon-sim-card.png", alt="icono sim card", srcset="images/header/submenu/icon-sim-card.png")
                            span.text ¿Qué es Chip Tuenti?
                        li.submenu__item: a(href="http://", target="_blank", rel="noopener noreferrer", class="link") 
                            span.icon: img(src="images/header/submenu/icon-sim-card.png", alt="icono sim card", srcset="images/header/submenu/icon-sim-card.png")
                            span.text ¿Cómo puedo ser Tuenti?
                        li.submenu__item: a(href="http://", target="_blank", rel="noopener noreferrer", class="link") 
                            span.icon: img(src="images/header/submenu/icon-sim-card.png", alt="icono sim card", srcset="images/header/submenu/icon-sim-card.png")
                            span.text Pasarme a Tuenti        
                                                                                              
                li.menu__item.has__child
                    a(href="http://", target="_blank", rel="noopener noreferrer", class="link accordion__tab") 
                        span.icon: img(src="images/header/icon-phone-add.png", alt="icono celular", srcset="images/header/icon-phone-add.png")
                        span.text Recarga
                        span.icon.icon--toggle: svg: use(xlink:href=iconPath+'chevron-down')
                    ul.submenu__list.accordion__panel
